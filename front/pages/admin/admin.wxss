/* pages/admin/admin.wxss */
@import "../setVip/setVip.wxss";
page{
  background: #f5f5f5;
}
.tabs{
  position: fixed;
  z-index: 9;
  width: 100%;
  height: 45px;
}
.tabs .van-dropdown-menu{
  border-bottom: 1rpx solid #ddd;
  height: 45px;
}
.tabs .van-dropdown-menu__title{
  font-size: 28rpx !important;
}
.tabs .van-dropdown-menu{
  box-shadow: none !important;
}
.container{
  padding: 40px 16rpx 120rpx 16rpx;
}
.list .item{
  position: relative;
}
.list .item .left{
  width: 180rpx;
}
.list .item .left .del{
  text-align: center;
  margin-top: 20rpx;
  text-decoration: underline;
}
.list .item .right{
  width: 480rpx;
}
.list .item .btns{
  position: absolute;
  right: 30rpx;
  top: 36%;
  transform: translateY(-50%);
}
.list .item .btns .btn{
  width: 160rpx;
  margin: 0;
  border-radius: 40rpx;
}
.bottom{
  width: 430rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  position: fixed;
  bottom: 30rpx;
  left: 160rpx;
  border-radius: 50rpx;
  font-size: 36rpx;
}
.dialog{
  margin-top: 20rpx;
}

.picker {
  display: flex;
}