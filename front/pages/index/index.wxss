 }
 .position{
  position: relative; 
  height: 100%;
 }

.logo-name{
  margin-bottom: 0;
}

.section .title{
  /* width: 100%; */
  margin-left: 5%;
  margin-top: 100rpx;
  margin-bottom: 20rpx;
  border-width: 0;
  border-left-width: 10rpx;
  border-style: solid;
  padding-left: 10rpx;
  line-height: 1;
  font-weight: 700;
  font-size: 36rpx;
}
.section .content image{
  width: 100%;
  overflow: hidden;
}
.intro{
  /* position: relative; */
  /* top: -10rpx; */
  border-radius: 10rpx;
  /* padding-bottom: 10rpx; */
  justify-content: space-between;
  background-repeat: no-repeat;
  /* background: #fff; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 46rpx;
  width:96%;
  margin-left:2%;
  /* box-shadow: #b0b0b0 0rpx 0rpx 20rpx; */
}
.intro  .address{
  font-size: 22rpx;
  color: #666;
  line-height: 30rpx;
  margin-left: 30rpx;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1; /* 超出几行省略 */
  overflow: hidden;
}
.intro .address .iconfont{
  display: inline-block;
  margin-right: 6rpx;
  font-size: 24rpx;
}
.intro .btns{
  display: flex;
}
.intro .btn{
  margin-left: 5rpx;
  margin-right: 45rpx;
  border-radius: 100%;
  /* width: 50rpx; */
  /* height: 50rpx; */
  text-align: center;
  /* line-height: 50rpx; */
  /* background: #fff; */
}
.banner swiper{
  width: 100%;
  height: 600rpx;
  overflow: hidden;
}
.banner image{
  width: 100%;
  height: 100%;
}
.toolbar{
  height: 666rpx;
  /* background: linear-gradient(#e66465, #9198e5); */
  width: 750rpx;
  /* padding-top: 1rpx; */
}
.toolbar .line1 {
  display: flex;
  justify-content: space-between;
  /* margin: 10rpx 2%; */
  /* margin-top: 12rpx; */
  padding: 0 2%;
}
.toolbar .line1 .td1 .tg{
  width: 320rpx;
  height: 200rpx;
  margin-bottom: 10rpx;
  background-size: 110% 150%;
  background-position-y:-30rpx;
  background-position-x:-45rpx;
  background-color: #ffffff;
  background-repeat: no-repeat;
}

.toolbar .line1 .right-box {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.toolbar .sub-item {
  width: 345rpx;
  height: 193rpx;
  background-size: cover;
  background-repeat:no-repeat;
  border-radius: 10rpx;
  /* box-shadow: #b0b0b0 0rpx 0rpx 20rpx; */
}

.toolbar .sub-item.slim {
  width: 345rpx;
  height: 145rpx;
  background-size: cover;
  background-repeat:no-repeat;
}

.toolbar .line1 .yuyue{
  width: 345rpx;
  height: 410rpx;
  background-size: cover;
  background-repeat:no-repeat;
  border-radius: 10rpx;
  /* box-shadow: #b0b0b0 0rpx 0rpx 20rpx; */
}
.toolbar .line2 {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 2% 30rpx 2%;
}

.margin-t-26 {
  margin-top: 26rpx;
}

